var counter = 0;

func addCount() -> $number {
    counter = counter + 1;
    return counter;
}

func loop($number: i) -> $void {
    if (i <= 0) {
        return;
    }

    loop(i - 1);
    if (i % 2 || (addCount() && i % 5 == 4)) {
        print("Added on " + i);
    }
}

loop(10);
print("Counter: " + counter);
